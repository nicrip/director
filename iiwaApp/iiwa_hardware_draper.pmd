
group "0.bot-lcm-tunnel" {

  cmd "0.remote-bot-tunnel" {
    exec = "ssh -t draperbender kuka-dev/drake/build/install/bin/bot-lcm-tunnel";
    host = "localhost";
  }

  cmd "1.local-bot-tunnel" {
    exec = "$DRAKE_BASE/build/install/bin/bot-lcm-tunnel 192.168.0.218";
    host = "localhost";
  }

  cmd "2.remote deputy" {
    exec = "ssh -t draperbender kuka-dev/drake/build/install/bin/bot-procman-deputy -n iiwarobotlab";
    host = "localhost";
  }
}

group "1.vision-drivers" {


  cmd "0.openni-driver" {
    exec = "$DIRECTOR_BUILD/install/bin/openni2-camera-lcm";
    host = "localhost";
  }

  cmd "0.apriltag-driver" {
    exec = "$DIRECTOR_BUILD/install/bin/drc-apriltags-driver --quiet=true --window=false --config=tri_iiwa_apriltags.yaml";
    host = "localhost";
  }

}

group "2.iiwa-drivers" {

  cmd "0.kuka-driver" {
    exec = "/home/momap/kuka-dev/iiwa-driver/build/kuka_driver";
    host = "localhost";
  }

  cmd "1.plan-runner" {
    exec = "$DRAKE_BASE/build/drake/examples/kuka_iiwa_arm/kuka_plan_runner";
    host = "localhost";
  }

  cmd "2.state-translator" {
    exec = "DRC_BASE=/none $DRAKE_BASE/build/install/bin/directorPython $DRAKE_BASE/drake/examples/kuka_iiwa_arm/kuka_iiwa_state_translator.py --director_config $DRAKE_BASE/drake/examples/kuka_iiwa_arm/director_config.json";
    host = "localhost";
  }
}

group "3.right-hand-driver" {

  cmd "0.right-hand-driver" {
    exec = "roslaunch reflex reflex_takktile.launch ";
    host = "localhost";
  }
  cmd "1.lcm-ros-bridge" {
    exec = "rosrun reflex takktile_command_server.py";
    host = "localhost";
  }

}

cmd "4.director-ik-app" {
  exec = "DRC_BASE=/none $DRAKE_BASE/build/install/bin/directorPython $DRAKE_BASE/drake/examples/kuka_iiwa_arm/director_ik_app.py  --director_config $DRAKE_BASE/drake/examples/kuka_iiwa_arm/director_config.json";
  host = "localhost";
}

cmd "5.director-camera-app" {
  exec = "$DIRECTOR_BUILD/install/bin/directorPython -m director.mainwindowapp --script iiwaManipApp.py --director-config $DRAKE_BASE/drake/examples/kuka_iiwa_arm/director_config.json --bot-config iiwaManip.cfg";
  host = "localhost";
}


cmd "5.bot-spy" {
  exec = "bash -c 'cd $DRAKE_BASE; ./my-bot-spy'";
  host = "localhost";
}

cmd "6.signal-scope-iiwa-command" {
  exec = "$SIGNAL_SCOPE_BASE/signal-scope $DRAKE_BASE/drake/examples/kuka_iiwa_arm/kuka_iiwa_signal_scope.py ";
  host = "localhost";
}
cmd "7.signal-scope-rhr-status" {
  exec = "$SIGNAL_SCOPE_BASE/signal-scope /home/momap/kuka-dev/rhr-driver/rhr_signal_scope.py ";
  host = "localhost";
}

cmd "8.grasp_demo" {
  exec = "cd /home/momap/catkin_ws/src/hardware_interfaces/reflex-ros-pkg-master/reflex/src; python object_pickup_iiwa_demo.py ";
